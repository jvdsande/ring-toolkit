{
  "name": "@ring-toolkit/pipeline",
  "version": "0.0.1",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "description": "Pipeline system for @web/dev-server and rollup setup",
  "license": "MIT",
  "repository": "https://github.com/jvdsande/ring-toolkit",
  "author": "Jérémie van der Sande <jeremie.vandersande@gmail.com>",
  "main": "./dist/index.js",
  "bin": {
