{
  "name": "ring-toolkit",
  "version": "0.0.5",
  "description": "All in one toolkit for web app development",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "rollup src/index.js --dir dist --format cjs --exports named --external true"
  },
  "repository": "https://github.com/jvdsande/ring-toolkit",
  "author": "Jérémie van der Sande <jeremie.vandersande@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "ring-toolkit": "bin.js"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@web/config-loader": "^0.1.3",
    "@web/dev-server": "^0.1.11",
    "@web/dev-server-rollup": "^0.3.3",
    "@web/test-runner": "^0.12.19",
    "cli": "^1.0.1",
    "depcheck": "^1.4.0",
    "fs-extra": "^9.1.0",
    "rollup": "^2.45.2",
    "sade": "^1.7.4"
  }
}
